import json
 
# строка которую будем парсить
json_string = """
{
        "title": "Title #1",
        "body": "Hello, World 1!"
}
"""
# распарсенная строка
parsed_string = json.loads(json_string)

k= parsed_string.keys()
n = len(k)
s = ""
i=0

while i < n:
    q = parsed_string.popitem()
    s = ("<" + q[0] + "> " + q[1] + "</" + q[0] + ">\n") + s
    i = i + 1

print(s)
f = open('text.html', 'w')
f.write(s)
f.close()
